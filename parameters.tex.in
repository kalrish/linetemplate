\leftmargin=@MARGINLEFT@
\rightmargin=@MARGINRIGHT@
\linerulethickness=@LINERULETHICKNESS@
\marginrulethickness=@MARGINRULETHICKNESS@
\interlineskip=@INTERLINESKIP@
